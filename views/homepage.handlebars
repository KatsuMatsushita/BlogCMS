<!-- Loop over each object in the posts array -->
{{#each posts as |post| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the span as HTML -->
      <!-- {{{get_emoji}}} -->
      <a href="/posts/{{post.id}}">{{post.title}}</a>
    </h2>
  </div>
</div>
{{/each}}
